<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /pae/api/
    
    # Don't rewrite files or directories
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    
    # Rewrite all other requests to index.php
    RewriteRule ^(.*)$ index.php [QSA,L]
</IfModule>
